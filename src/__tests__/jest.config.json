{
  "globals": {
    "ts-jest": {
      "tsConfig": "src/config/tsconfig.json",
      "diagnostics": {
        "ignoreCodes": [151001]
      }
    }
  },
  "moduleFileExtensions": ["ts", "tsx", "js"],
  "transform": {
    "\\.(ts|tsx)$": "ts-jest"
  },
  "testRegex": "__tests__/.*\\.(ts|tsx|js)$"
}
