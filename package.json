{
  "bin": {
    "ultiscss": "./src/config/external.js"
  },
  "main": "./dist/EntryPoint.js",
  "name": "ultiscss",
  "version": "1.2.1",
  "description": "SCSS Component Builder and Validator",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "src/config/make.js build",
    "clean": "src/config/make.js clean",
    "coverage": "nyc yarn test",
    "test": "src/config/make.js test",
    "version_click": "node src/config/make.js version_click"
  },
  "author": "stephen.d.francis@gmail.com",
  "repository": {
    "type": "git",
    "url": "git@github.com:stephenfrancis/ultiscss.git"
  },
  "license": "ISC",
  "sideEffects": false,
  "ultimake_settings": {
    "exec_on_version_click": "npm publish --access public"
  },
  "ultiscss_settings": {
    "source_dir": "src/scss",
    "target_dir": "build"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.13",
    "@types/ejs": "^3.0.6",
    "@types/jest": "^26.0.23",
    "bootstrap": "^5.0.2",
    "cheerio": "^1.0.0-rc.2",
    "css": "^3.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "jest": "^27.0.6",
    "jquery": "^3.5.1",
    "node-sass": "^6.0.1",
    "ts-jest": "^27.0.3",
    "typescript": "4.3.5",
    "ultimake": "0.7.1"
  },
  "devDependencies": {
    "nyc": "^15.0.0"
  }
}
